{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "latex-workshop.latexindent.args": ["-l", "-c", "%DIR%/", "%TMPFILE%"],
  "latex-workshop.latexindent.path": "latexindent",
  "latex-workshop.latex.recipes": [
    {
      "name": "compile",
      "tools": ["pdflatex-draft", "pdflatex"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "pdflatex-draft",
      "command": "pdflatex",
      "args": [
        "-draftmode",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-shell-escape",
        "%DOC%"
      ]
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-shell-escape",
        "%DOC%"
      ]
    },
    {
      "command": "bibtex",
      "args": ["%DOCFILE%"]
    }
  ],
  "licenser.author": "Gabriele Proietti Mattia <gabry.gabry@hotmail.it>; Alexandru Daniel Tufa <alex.tufa94@gmail.com>",
  "licenser.license": "CC-BY-NC-SA-4",
  "licenser.projectName": "Fibers (Docs)"
}
